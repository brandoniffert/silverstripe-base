{
  "name": "brandoniffert/silverstripe-base",
  "type": "silverstripe-recipe",
  "description": "A base SilverStripe setup",
  "require": {
    "php": "^8.0 || ^8.1",
    "silverstripe/recipe-plugin": "^1.2",
    "silverstripe/recipe-cms": "~4.13.0@stable",
    "silverstripe/login-forms": "~4.8.0@stable",
    "cakephp/chronos": "^2.3",
    "dnadesign/silverstripe-elemental": "^4",
    "firebrandhq/silverstripe-phonelink": "^2.1",
    "jonom/focuspoint": "^4.0",
    "kinglozzer/metatitle": "^2.0",
    "ryanpotter/silverstripe-cms-theme": "^3.4",
    "silverstripe/redirectedurls": "^2.1",
    "silverware/calendar": "^1.1",
    "symbiote/silverstripe-gridfieldextensions": "^3.6",
    "unclecheese/display-logic": "^2.0",
    "wilr/silverstripe-googlesitemaps": "^2.2",
    "axllent/silverstripe-trailing-slash": "^2.2",
    "wedevelopnl/silverstripe-webp-images": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/src/"
    },
    "classmap": [
      "app/src/Page/Page.php",
      "app/src/Page/PageController.php"
    ]
  },
  "extra": {
    "project-files": [
      "app/_config/*",
      ".env.example",
      ".graphql-generated/*"
    ],
    "public-files": [
      "assets/*",
      "favicon.ico"
    ],
    "resources-dir": "_resources",
    "project-files-installed": [
      "app/.htaccess",
      "app/_config.php",
      "app/_config/mimevalidator.yml",
      "app/_config/mysite.yml",
      "app/src/Page.php",
      "app/src/PageController.php"
    ],
    "public-files-installed": [
      ".htaccess",
      "index.php",
      "web.config"
    ],
    "expose": [
      "app/cms",
      "themes/app/dist"
    ]
  },
  "config": {
    "process-timeout": 600,
    "allow-plugins": {
      "composer/installers": true,
      "silverstripe/recipe-plugin": true,
      "silverstripe/vendor-plugin": true
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
